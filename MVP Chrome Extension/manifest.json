{
    "name" : "Bias Finder",
    "version" : "0.0.0.1",
    "description": "Find other sources with different biases",
    "manifest_version": 2,
    "permissions": [
        "tabs"
     ],
     "icons": { "16": "icons/icon16.png",
        "48": "icons/icon48.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["dom.js"]
        }
    ]
}
